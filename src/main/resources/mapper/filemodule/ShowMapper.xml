<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.easycloudstorage.mapper.filemodule.ShowMapper">
    <select id="normalFileList" resultType="com.easycloudstorage.pojo.NormalFile">
        select * from normalFile;
    </select>

    <select id="directoryList" resultType="Directory">
        select * from directory;
    </select>
</mapper>